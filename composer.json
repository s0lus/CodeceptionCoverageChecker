{
  "name": "s0lus/codeception-coverage-checker",
  "description": "Extension for codeception. Can be used to fail tests if they are under the coverage threshold",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "codeception",
    "code coverage",
    "coverage",
    "extension",
    "code checker",
    "check"
  ],
  "authors": [
    {
      "name": "Harm Zeinstra",
      "email": "harmzeinstra@hotmail.com",
      "homepage": "https://mhzeinstra.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "codeception/codeception": "^5"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "dg/bypass-finals": "*"
  },
  "autoload": {
    "psr-4": {
      "CoverageChecker": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Nebulosar\\": "tests/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "composer dump-autoload -o"
    ]
  }
}
